# docker network name
DOCKER_NETWORK_NAME=jupyter-network

# grader service image
DOCKER_GRADER_IMAGE={{docker_illumidesk_grader_image}}

# deployment working directory
ILLUMIDESK_DIR={{working_dir}}

# api token should be equal to value set for jupyterhub
JUPYTERHUB_API_TOKEN={{jhub_api_token}}

# base url for jupyterhub application
JUPYTERHUB_BASE_URL={{base_url}}

# jupyterhub api url using proxy name and the proxy's external port
JUPYTERHUB_API_URL=http://reverse-proxy:8000{{base_url}}/hub/api

# local path to save the dynamic config
JUPYTERHUB_CONFIG_PATH=/srv/jupyterhub
JUPYTERHUB_SERVICE_NAME=jupyterhub

# mount root directory 
MNT_ROOT={{mnt_root}}

# grader uid/gid to restrict access
MNT_ROOT={{mnt_root}}
NB_UID=10001
NB_GID=100

SHARED_FOLDER_ENABLED={{shared_folder_enabled}}

# Postgres settings for NBgrader
POSTGRES_NB_HOST={{postgres_nb_host}}
POSTGRES_NB_USER={{postgres_nb_user}}
POSTGRES_NB_PASSWORD={{postgres_nb_password}}
POSTGRES_NB_DB={{postgres_nb_dbname}}
