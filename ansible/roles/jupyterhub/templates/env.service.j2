# api token should be equal to value set for jupyterhub
JUPYTERHUB_API_TOKEN={{jhub_api_token}}

# jupyterhub api url using proxy name and the proxy's external port
JUPYTERHUB_API_URL=http://reverse-proxy:8000{{base_url}}/hub/api

# jupyterhu client id used with oauth2 setup
JUPYTERHUB_CLIENT_ID=service-{{course_id}}

# jupyterhu user used with oauth2 setup
JUPYTERHUB_USER=grader-{{course_id}}

# Grader service prefix
JUPYTERHUB_SERVICE_PREFIX=/services/{{course_id}}

# grader user/uid/gid to restrict access
NB_USER=grader-{{course_id}}
NB_UID=10001
